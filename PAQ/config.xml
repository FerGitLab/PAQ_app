<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webinformatica.PAQ" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PAQ</name>
    <description>
        Aplicaci√≥n para envio de paquetes
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Wen Informatica
    </author>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.CameraLauncher" />
    </feature>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.CameraLauncher" />
    </feature>
    <content src="validador.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Descripcion" />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="Foto" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-filechooser" spec="^1.2.0" />
    <plugin name="cordova-plugin-x-toast" spec="^2.7.2" />
    <plugin name="phonegap-plugin-local-notification" spec="^1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-barcodescanner" spec="^0.7.4" />
    <engine name="android" spec="^8.0.0" />
    <engine name="ios" spec="^5.0.0" />
    <engine name="windows" spec="^6.0.1" />
</widget>
